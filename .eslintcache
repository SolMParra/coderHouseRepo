\n        \n        db.collection('productos').where(\"category\", \"==\", id).get()\n        .then(docs =>{\n            let arr = [];\n            docs.forEach(doc => {\n                arr.push({id: doc.id, data: doc.data()});\n               \n            })\n\n            setItems(arr);\n        })\n        .catch(e => console.log(e));\n    }    \n\n    useEffect(() => {\n        id ? getProductsFromDBCategory() : getProductsFromDB()\n\n            console.log(items);\n        \n    }, [id]);   \n\n\n    return (\n\n        <>\n                         <div className=\"row col-12\">\n                     {\n                         items.length == 0 ?\n                         \n                         <h5 className=\"col-12 text-center mt-5\" >No existen productos para esta categoria</h5> \n                           :\n                                              \n                         items.length > 0 ?\n                         \n                            items.map((item, index)=> (\n   \n                            <Producto \n                            key={item.id} \n                            id={item.id}\n                            nombre={item.data.titulo} \n                            precio={item.data.precio} \n                            stock={item.data.stock}\n                            img={item.data.img}/>\n                            \n                            ))\n                         :\n                         <h5 className=\"col-12 text-center mt-5\" >cargando items...</h5>\n                     }\n\n                  \n                </div>\n        </>\n\n    )\n}\n\n\nexport default ItemList;","/Users/sparra/Desktop/solproyect-app/src/Components/Product/producto.jsx",["88","89","90","91"],"/Users/sparra/Desktop/solproyect-app/src/Components/ItemCount/ItemCount.jsx",[],"/Users/sparra/Desktop/solproyect-app/src/Components/WidgetCart/WidgetCart.jsx",[],"/Users/sparra/Desktop/solproyect-app/src/Components/ItemDetail/itemD