;\n\n\nfunction Navbar(imagen) {\n\n    const [cate, setCate] = useState([]);\n    const [data, setData] = useContext(Store);\n    const db = getFirestore();\n\n    const getCategoriasFromDB = () => {\n\n        db.collection('categorias').orderBy(\"nombre\", \"asc\").get()\n\n            .then(docs => {\n\n                let arra = [];\n                docs.forEach(doc => {\n                    arra.push({ id: doc.id, data: doc.data() })\n                    console.log(doc.id);\n\n                })\n\n                setCate(arra);\n\n            })\n            .catch(e => console.log(e))\n    }\n\n    useEffect(() => {\n        getCategoriasFromDB();\n\n    }, []);\n\n    return (\n\n        <>\n            <nav className=\"navbar fixed-top  navbar-expand-lg navbar-light navbar-fondo \">\n                <a className=\"navbar-brand\" href=\"#\"><Link to={'/'}><img src={imagen.src} alt=\"ImagenPrincipal\" />  </Link></a>\n\n                {\n                    cate.map((item, index) => (\n\n\n                        <Link key={item.id} className=\"nav-link\" to={`/category/${item.data.nombre}`}>{item.data.nombre}</Link>                       \n\n                    ))\n                }\n\n                <Carro />\n\n            </nav>\n            \n        </>\n\n\n    );\n\n\n}\n\n\nexport default Navbar;","/Users/sparra/Desktop/solproyect-app/src/Components/ListProdHome/listadoProdHome.jsx",[],"/Users/sparra/Desktop/solproyect-app/src/Components/Global/carro.jsx",["83","84"],"import Widgetcart from '../WidgetCart/WidgetCart';\nimport { useState, useContext } from 'react';\nimport './carro.css';\nimport {Store} from '../../store';\n\n\nfunction Carro(){\n\n    const [data, setData] = useContext(Store);\n\n    const [verWidgetCar, setVerWidgetCar] = useState(false);\n\n    const openWidgetCard = () => {\n\n        setVerWidgetCar(!verWidgetCar);\n\n    }\n\n    return(\n\n        <>\n            <div className={`${data.cantidad > 0 ? 'd-block' : 'd-none'}`